{
  "name": "dynamic-mapper",
  "version": "6.1.5",
  "author": "christof.strack@cumulocity.com, stefan.witschel@cumulocity.com",
  "description": "Cumulocity Dynamic Mapper UI to map data from any data source to Cumulocity and vice-versa in a zero-code/low-code way. When using an AI Provider code and mappings can be generated.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint --ext .ts,.component.html .",
    "cy:run": "cypress run"
  },
  "license": "Apache-2.0",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/openai": "^1.3.24",
    "@angular/cdk": "^20.2.14",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@c8y/bootstrap": "1023.17.6",
    "@c8y/client": "1023.17.6",
    "@c8y/ngx-components": "1023.17.6",
    "@c8y/style": "1023.17.6",
    "ai": "^4.3.19",
    "echarts": "^5.6.0",
    "jsonata": "^2.1.0",
    "monaco-editor": "^0.53.0",
    "ngx-bootstrap": "20.0.2",
    "ngx-echarts": "^18.0.0",
    "rxjs": "7.8.2",
    "tslib": "^2.3.0",
    "vanilla-jsoneditor": "^3.11.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.12",
    "@angular/cli": "^20.3.12",
    "@angular/compiler-cli": "^20.3.0",
    "@c8y/devkit": "1023.17.6",
    "@c8y/options": "1023.17.6",
    "@types/jasmine": "~5.1.0",
    "cumulocity-cypress": "^1.4.1",
    "cypress": "^15.8.2",
    "cypress-slow-down": "^1.3.1",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "less": "^4.2.0",
    "typescript": "~5.9.2"
  }
}